    "lib/franky.js",
    "lib/franky.min.js",
    "lib/franky.min.map",
    "LICENSE"
  ],
  "main": "franky.js",
  "version": "1.0.27",
  "devDependencies": {
    "docco": "0.6.x",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-uglify": "^0.6.0",
    "grunt-docco": "~0.3.3",
    "grunt-gh-pages": "^0.9.1",
    "karma": ">=0.12.16",
    "karma-jasmine": "0.1.1",
    "karma-phantomjs-launcher": ">=0.1.4",
    "requirejs": "2.1.14",
    "uglify-js": "2.4.x"
  },
  "scripts": {
    "pretest": "npm run-script build",
    "prestart": "npm run-script build",
    "doc": "npm install && grunt doc",
    "test": "./node_modules/karma/bin/karma start karma.conf.js --log-level error",
    "build": "npm install && grunt"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/ikondrat/franky/master/LICENSE"
    }
  ],
  "jshintConfig": {
    "globals": {
      "RpsGame": true,
      "mylib": "true",
      "describe": true,
      "it": true,
      "expect": true
    },
    "bitwise": true,
    "browser": true,
    "jquery": true,
    "node": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": false,
    "indent": 4,
    "latedef": true,
    "noarg": true,
    "noempty": true,
    "undef": true,
    "unused": false,
    "strict": false,
    "trailing": true,
    "maxparams": 3,
    "maxdepth": 5,
    "maxlen": 180,
    "laxcomma": true,
    "sub": true,
    "white": false,
    "newcap": false
  }
}
